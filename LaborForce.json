{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "width": 400,
  "height": 300,
  "data": {
    "url": "https://raw.githubusercontent.com/varxem/FIT3179/main/LaborForce2016.csv"
  },
  "title": "Number of Females in Labour Force",
  "mark": {"type": "line", "point": true, "tooltip": true},
  "encoding": {
    "x": {"field": "Year", "title": null},
    "y": {
      "field": "Total",
      "type": "quantitative",
      "scale": {"domain": [10000000, 50000000]},
      "title": "Number of Females"
    },
    "color": {"field": "Country Name", "type": "nominal", "title": "Country"}
  },
  "config": {}
}